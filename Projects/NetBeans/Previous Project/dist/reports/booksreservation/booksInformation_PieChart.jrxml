	<parameter name="title" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT DISTINCT
b.idbooks,
b.barcode,
b.title,
b.pages,
b.description,
b.`language`,
b.cost,
b.purchasedcost,
b.copynumber,
b.acquireddate,
b.isborrowable,
b.category_name,
Count(lbb.books_idbooks)
FROM
logsborrowedbooks AS lbb
LEFT JOIN books AS b ON lbb.books_idbooks = b.idbooks
WHERE
lbb.borroweddate BETWEEN $P{dateFrom} AND $P{dateTo}

GROUP BY
b.idbooks,
b.barcode,
b.title,
b.pages,
b.description,
b.`language`,
b.cost,
b.purchasedcost,
b.copynumber,
b.acquireddate,
b.isborrowable,
b.category_name
ORDER BY
b.category_name ASC,
Count(lbb.books_idbooks) DESC]]>
	</queryString>
